{
    "compilerOptions": {
        "allowJs": true,
        "checkJs": true,
        "esModuleInterop": true,
        "module": "esnext",
        "moduleResolution": "node",
        "noEmit": true,
        "noLib": true,
        "paths": {
            "http://*": [
                "../../.deno/deps/http/*"
            ],
            "https://*": [
                "../../.deno/deps/https/*"
            ],
        },
        "plugins": [
            {
                "name": "deno_ls_plugin"
            }
        ],
        "pretty": true,
        "resolveJsonModule": true,
        "target": "esnext"
    },
    "include": [
        "./**/*.ts"
    ]
}